{
  "name": "uselessglaringprojects",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "test": "jest --coverage"
  },
  "dependencies": {
    "babel-traverse": "6.26.0",
    "babel-types": "6.26.0",
    "babylon": "6.18.0",
    "@babel/core": "7.5.4",
    "@babel/parser": "7.5.0"
  },
  "devDependencies": {
    "babel-plugin-tester": "6.4.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-jest": "22.6.3",
    "eslint-plugin-prettier": "3.1.0",
    "husky": "2.3.0",
    "jest": "24.8.0",
    "lint-staged": "8.1.7",
    "prettier": "1.17.1"
  },
  "keywords": [],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "eslint --fix",
        "git add"
      ],
      "src/**/*.json": [
        "prettier --write",
        "git add"
      ],
      "*.md": [
        "prettier --write",
        "git add"
      ]
    },
    "ignore": [
      "**/*.min.js"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src/"
    ],
    "collectCoverageFrom": [
      "!**/__specs__/**"
    ]
  }
}
